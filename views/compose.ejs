<h1>COMPOSE</h1>
 <form method="post" action ="/compose">
  <div class="form-group">
    <label for="composeTitle">Title</label>
    <input class="form-control" type="text" name="composeTitle" autocomplete="off" >
  </div>
  <div class="form-group">
    <label for="composePost">Post</label>
     <textarea class="form-control" name="composePost" cols="50" rows="5"></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="Publish">Publish</button>
</form>

